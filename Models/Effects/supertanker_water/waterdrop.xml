<?xml version="1.0" encoding="UTF-8"?>
<!-- New version May, 07, 2009 by 102nd-YU-Nitro -->
<PropertyList>
  <!-- OSG Particles -->
  <particlesystem>
    <name>waterdrop</name>
    <offsets>
      <x-m>0.000</x-m>
      <y-m>0.000</y-m>
      <z-m>-0.000</z-m>
      <roll-deg>0.000</roll-deg>
      <pitch-deg>0.000</pitch-deg>
      <heading-deg>0.000</heading-deg>
    </offsets>
    <texture>water.png</texture>
    <condition>
    <property>controls/switches/water-drop</property>
    <!--
      <and>
        <property>sim/multiplay/generic/bool[0]</property>
        <not>
          <property>sim/multiplay/generic/bool[13]</property>
        </not>
      </and>-->
    </condition>
    <emissive>false</emissive>
    <lighting>false</lighting>
    <align>billboard</align> <!-- billboard / fixed -->
    <attach>world</attach> <!-- world / local-->
    <placer>
      <type>point</type> <!-- sector / segments / point -->
    </placer>
    <shooter>
      <theta-min-deg>10</theta-min-deg>
      <theta-max-deg>120</theta-max-deg>
      <phi-min-deg>-180</phi-min-deg>
      <phi-max-deg>180</phi-max-deg>
      <speed-mps>
        <value>10</value>
        <spread>10</spread>
      </speed-mps>
      <rotation-speed>
        <x-min-deg-sec>5</x-min-deg-sec>
        <y-min-deg-sec>5</y-min-deg-sec>
        <z-min-deg-sec>5</z-min-deg-sec>
        <x-max-deg-sec>60</x-max-deg-sec>
        <y-max-deg-sec>60</y-max-deg-sec>
        <z-max-deg-sec>60</z-max-deg-sec>
      </rotation-speed>
    </shooter>
    <counter>
      <particles-per-sec>
        <value>50</value>
        <spread>1</spread>
      </particles-per-sec>
    </counter>
    <particle>
      <start>
        <color>
          <red>
            <value>1</value>
          </red>
          <green>
            <value>.5</value>
          </green>
          <blue>
            <value>.5</value>
          </blue>
          <alpha>
            <value>0.3</value>
          </alpha>
        </color>
        <size>
          <value>1</value>
        </size>
      </start>
      <end>
        <color>
          <red>
            <value>1</value>
          </red>
          <green>
            <value>.6</value>
          </green>
          <blue>
            <value>.6</value>
          </blue>
          <alpha>
            <value>0.001</value>
          </alpha>
        </color>
        <size>
          <value>400</value>
        </size>
      </end>
      <life-sec>
        <value>10</value>
      </life-sec>
      <mass-kg>500</mass-kg>
      <radius-m>1.0</radius-m>
    </particle>
    <program>
      <fluid>water</fluid> <!-- air / water -->
      <gravity>true</gravity>
      <wind>true</wind>
    </program>
  </particlesystem>
</PropertyList>